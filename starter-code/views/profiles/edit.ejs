<form action="/profile/<%=id %>" method="post">
  <label for="username">Username</label><br>
  <input type="text" id="username" name="username" value="<%=username %>" disabled><br>

  <label for="name">Name</label><br>
  <input type="text" id="name" name="name" value="<%=name %>"><br>
  <label for="email">Email</label><br>
  <input type="email" id="email" name="email" value="<%=email %>"><br>
  <label for="jobTitle">jobTitle</label><br>
  <input type="text" id="jobTitle" name="jobTitle" value="<%=jobTitle %>"><br>

  <label for="imageUrl">image</label><br>
  <input type="text" id="imageUrl" name="imageUrl" value="<%=imageUrl %>"><br>

  <label for="company">company</label><br>
  <input type="text" id="company" name="company" value="<%=company %>"><br>


  <br>
  <button> Modify user</button>
</form>
<a href="/"><button>BACK TO MENU</button></a>
